{
  "name": "arq",
  "version": "1.1.0",
  "description": "Advanced Request Queue - High-performance task processing system",
  "author": "fukkingsnow",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "build:watch": "nest build --watch",
    "start:dev:opt": "npm run build:watch & npm run start:dev",
    "build:prod": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "commit:src": "git add src/ && git commit -m",
    "deploy:optimized": "npm run build:prod && git push",
    "lint": "eslint src --fix",
    "lint:strict": "eslint src --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:frontend": "cd src/frontend && npm install && npm run build",
    "copy:frontend": "mkdir -p dist && cp -r src/frontend dist/frontend",
    "audit:fix": "npm audit fix --force",
    "audit:strict": "npm audit --audit-level=moderate",
    "quality:fix": "npm run lint -- --fix && npm audit fix",
    "build:webpack": "nest build"
  },
  "dependencies": {
    "@nestjs/bull": "11.0.4",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/typeorm": "10.0.1",
    "@nestjs/websockets": "10.3.0",
    "@octokit/rest": "20.0.2",
    "axios": "1.13.5",
    "bcrypt": "5.1.1",
    "bull": "4.16.5",
    "bullmq": "5.13.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "dotenv": "17.2.3",
    "eventemitter2": "6.4.9",
    "express": "5.2.1",
    "helmet": "7.1.0",
    "ioredis": "5.3.2",
    "joi": "17.11.0",
    "nodemailer": "6.9.7",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "pg": "8.10.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "socket.io": "4.7.2",
    "swagger-ui-express": "5.0.0",
    "typeorm": "0.3.17",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.3.0",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/multer": "1.4.11",
    "@types/node": "22.0.0",
    "@types/nodemailer": "6.4.14",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "prettier": "3.3.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4"
  }
}
